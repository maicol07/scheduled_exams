{
    "name": "maicol07/scheduledexams",
    "type": "project",
    "description": "Scheduled Exams",
    "keywords": [
        "scheduled",
        "exams"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "fruitcake/laravel-cors": "^2",
        "guzzlehttp/guzzle": "^7",
        "inertiajs/inertia-laravel": "^0",
        "innocenzi/laravel-vite": "^0",
        "laravel/framework": "^8",
        "laravel/sanctum": "^2",
        "laravel/tinker": "^2",
        "maicol07/laravel-json-api-resource": "^2",
        "nette/utils": "^3",
        "parsedown/laravel": "^1",
        "riverskies/laravel-mobile-detect": "^1",
        "tightenco/ziggy": "^1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.5",
        "nunomaduro/collision": "^5.10",
        "roave/security-advisories": "dev-latest",
        "kkomelin/laravel-translatable-string-exporter": "^1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
